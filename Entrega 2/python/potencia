// Potencia
DATA:
	RES  1 // var1
	RES_Parcial 0
	i 0
	I 0
	F 0
CODE:
	MOV (F),B
	MOV (I),A
	
Potencia:
	MOV A,(RES)
	MOV (i),A
	JMP Multiplicacion
	MOV A,(RES_Parcial)
	MOV (Res),A
	MOV A,0
	MOV (RES_Parcial),A
	MOV A,(F)
	SUB A,1
	MOV (F),A
	CMP A,0
	JGT Potencia


Multiplicacion:
	MOV A,(RES_Parcial)
	ADD A,(I)
	MOV (RES_Parcial),A
	MOV A,(i)
	SUB A,1
	MOV (i),A
	CMP A,0
	JNE Multiplicacion